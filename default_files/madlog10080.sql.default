select
@Datetime,
'10080',
instance,
sum(warns),
sum(failedData),
sum(manyTimeOut),
sum(fallingBehind),
max(maxFalling),
sum(frozenScreen),
sum(wsTimeout),
sum(wsConnClosed),
sum(noInject),
sum(noPogoStart),
sum(failedScreenshot),
sum(noPTC),
sum(failedTopApp),
sum(pogoRestartFail),
sum(screenFailure),
sum(failedBoxClear),
sum(itemDelUnknown),
sum(noActiveArea),
sum(noMainScreen),
sum(noLocAccess),
sum(noStopProcess),
sum(failStopOpen),
sum(softban),
sum(questFull),
sum(foundGym),
sum(noWalkerConfig),
sum(noStop),
sum(dupMac),
sum(failStopSpin),
sum(noLogin),
sum(failPostMove)
from pogodb.warning
where datetime >= date(curdate() - interval weekday(curdate()) + 7 day) and RPL = 1440
group by instance
;
